# bash aliases
if [ -f ~/.CheemaFy/installed ]; then
    . ~/programs/CheemaFy/myPlugins/bash_extended/bashrc
fi


if [ -f ~/.CheemaFy/installed_mac ]; then
    . ~/programs/CheemaFy/myPlugins/bash_extended/bashrc
    if [ -f $(brew --prefix)/etc/bash_completion ]; then
        . $(brew --prefix)/etc/bash_completion
    fi
fi
